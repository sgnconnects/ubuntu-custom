<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="audio/x-spc">
        <comment>Super Nintendo Sound File</comment>
        <glob pattern="*.spc"/>
        <magic priority="50">
            <match value="SNES-SPC700 Sound File Data" type="string" offset="0" />
        </magic>
        <icon name="audio-x-generic"/>
    </mime-type>
</mime-info>
